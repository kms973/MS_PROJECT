<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.global.ex.mapper.CartMapper">


   <select id="listCart" resultType="CartVO">
    <![CDATA[
      select * from ms_cart
   ]]>    
    </select>

    <delete id="delete">
    <![CDATA[
      delete from ms_cart where product_name = #{product_name}
   ]]>    
    </delete>
    
    <insert id="insert">
    <![CDATA[
	INSERT INTO ms_cart (username, product_img, product_name, price, stock_quantity VALUES (#{username},#{product_img} , #{product_name},#{price},#{stock_quantity}))
   ]]>    
    </insert>    

</mapper>